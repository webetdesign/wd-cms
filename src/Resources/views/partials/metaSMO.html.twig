<!-- Twitter Card data -->
<meta name="twitter:card" content="{{ object.twitterCard }}">
<meta name="twitter:site" content="{{ object.twitterSite }}">
<meta name="twitter:title" content="{{ object.twitterTitle }}">
<meta name="twitter:description" content="{{ object.twitterDescription }}">
<meta name="twitter:creator" content="{{ object.twitterCreator }}">
{#Twitter Summary card images must be at least 120x120px #}
<meta name="twitter:image" content="{{ object.twitterImage }}">

<!-- Open Graph data -->
<meta property="og:title" content="{{ object.fbTitle }}" />
<meta property="og:type" content="{{ object.fbType }}" />
<meta property="og:url" content="{{ object.fbUrl }}" />
<meta property="og:image" content="{{ object.fbImage }}" />
<meta property="og:description" content="{{ object.fbDescription }}" />
<meta property="og:site_name" content="{{ object.fbSiteName }}" />
<meta property="fb:admins" content="{{ object.fbAdmins }}" />
