<!-- Twitter Card data -->
{% if object is defined and cms_render_seo_smo_value(object, 'twitterCard') is not empty %}
    <meta name="twitter:card" content="{{ cms_render_seo_smo_value(object, 'twitterCard') }}">
{% else %}
    <meta name="twitter:card" content="{{ cms_render_seo_smo_value(page, 'twitterCard') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'twitterSite') is not empty %}
    <meta name="twitter:site" content="{{ cms_render_seo_smo_value(object, 'twitterSite') }}">
{% else %}
    <meta name="twitter:site" content="{{ cms_render_seo_smo_value(page, 'twitterSite') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'twitterTitle') is not empty %}
    <meta name="twitter:title" content="{{ cms_render_seo_smo_value(object, 'twitterTitle') }}">
{% else %}
    <meta name="twitter:title" content="{{ cms_render_seo_smo_value(page, 'twitterTitle') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'twitterDescription') is not empty %}
    <meta name="twitter:description" content="{{ cms_render_seo_smo_value(object, 'twitterDescription') }}">
{% else %}
    <meta name="twitter:description" content="{{ cms_render_seo_smo_value(page, 'twitterDescription') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'twitterCreator') is not empty %}
    <meta name="twitter:creator" content="{{ cms_render_seo_smo_value(object, 'twitterCreator') }}">
{% else %}
    <meta name="twitter:creator" content="{{ cms_render_seo_smo_value(page, 'twitterCreator') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'twitterImage') is not empty %}
    <meta name="twitter:image" content="{{ cms_render_seo_smo_value(object, 'twitterImage') }}">
{% else %}
    <meta name="twitter:image" content="{{ cms_render_seo_smo_value(page, 'twitterImage') }}">
{% endif %}

<!-- Open Graph data -->
{% if object is defined and cms_render_seo_smo_value(object, 'ogTitle') is not empty %}
    <meta property="og:title" content="{{ cms_render_seo_smo_value(object, 'ogTitle') }}">
{% else %}
    <meta property="og:title" content="{{ cms_render_seo_smo_value(page, 'ogTitle') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'ogType') is not empty %}
    <meta property="og:type" content="{{ cms_render_seo_smo_value(object, 'ogType') }}">
{% else %}
    <meta property="og:type" content="{{ cms_render_seo_smo_value(page, 'ogType') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'ogImage') is not empty and cms_render_seo_smo_value(object, 'ogImage')|wd_media_path() is not empty %}
    <meta property="og:image" content="{{ absolute_url(cms_render_seo_smo_value(object, 'ogImage')) }}">
{% else %}
    {% if cms_render_seo_smo_value(page, 'ogImage')|wd_media_path() is not empty %}
        <meta property="og:image" content="{{ absolute_url(cms_render_seo_smo_value(page, 'ogImage')) }}">
    {% endif %}
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'ogDescription') is not empty %}
    <meta property="og:description" content="{{ cms_render_seo_smo_value(object, 'ogDescription') }}">
{% else %}
    <meta property="og:description" content="{{ cms_render_seo_smo_value(page, 'ogDescription') }}">
{% endif %}

{% if object is defined and cms_render_seo_smo_value(object, 'ogSiteName') is not empty %}
    <meta property="og:site_name" content="{{ cms_render_seo_smo_value(object, 'ogSiteName') }}">
{% else %}
    <meta property="og:site_name" content="{{ cms_render_seo_smo_value(page, 'ogSiteName') }}">
{% endif %}

<meta property="og:url" content="{{ app.request.schemeAndHttpHost ~ app.request.requestUri }}">
