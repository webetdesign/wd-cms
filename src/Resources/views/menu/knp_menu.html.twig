{% extends 'knp_menu.html.twig' %}

{%- block linkElement -%}{% import _self as knp_menu %}<a href="{{ item.uri }}"{{ knp_menu.attributes(item.linkAttributes) }}>{% if item.getExtra('icon_class') is not null %}<i class="{{ item.getExtra('icon_class') }}"></i>{% endif %}{{ block('label') }}</a>{%- endblock -%}

{%- block spanElement -%}{% import _self as knp_menu %}<span{{ knp_menu.attributes(item.labelAttributes) }}>{% if item.getExtra('icon_class') is not null %}<i class="{{ item.getExtra('icon_class') }}"></i>{% endif %}{{ block('label') }}</span>{%- endblock -%}
